      sisters[1] = ["MICKRIG AFFEN IN HÖHLE?! \nAFFE HIRN GUT AUF BROT!!",160];
      sisters[2] = ["SCHWACH BRUDER GEWESEN BESIEGT VON NACKT AFFEN. \nGEWESEN GLEICHE AFFEN?!     \n\nBRUDER MÜSSEN GERÄCHT!!",160];
      sisters[3] = ["SCHIMPS BESIEGEN BRUDER?! \nSCHIMPS MÜSSEN SEIN SCHMETTER WIE WASSERMELONE!!",160];
      sisters[4] = ["ZYKLOP SCHWESTERN VIEL MEHR STARK ALS KLEIN BRUDER!!          \n\nBRUDER ALS KIND AUF AUGE GEFALLEN!!",160];
      sisters[5] = ["ZYKLOP BRAUCHEN LASER AUGE OPERATION NACH KAMPF.",160];
      sisters[6] = ["ZYKLOP GEHEN JETZT ZU GROSSE HÖHLE AN HIMMEL...",160];
      sisters[7] = ["AFFEN TÖTEN SCHWESTER UND MACHEN ZU HACKFLEISCH!!             \n\nAFFEN MUSS JETZT SCHMETTERN!!",160];
      sisters[8] = ["ZYKLOP GEHEN JETZT ZU FAMILIE.        \n\nZUSAMMEN FÜR IMMER...",160];
      sisters[9] = ["ZYKLOP JETZT ARBEITEN FÜR AFFEN, ABER ZYKLOP TRAUM BESTEHEN FORT.           \n\nZYKLOP DÜRFEN SCHMETTERN KLEIN FLEISCHKLUMPEN!!",160];
      sisters[10] = ["AFFEN NICHT SO SCHLECHT!! \nZYKLOP JETZT FREI VON HÖHLE UND SEHEN WELT!!           \n\nVITAMIN D AUFGEFÜLLT!!",160];
