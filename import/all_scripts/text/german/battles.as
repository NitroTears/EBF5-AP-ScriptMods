      sisters[12] = ["Wir haben schon einen Zyklopen erledigt, da kriegen wir auch noch zwei weitere klein!",2,34];
      sisters[13] = ["Oh-oh, sind die mit dem Anderen verwandt?!       \nDann wollen sie vermutlich Rache!",24,54];
      sisters[14] = ["Also, nur, falls das noch nicht klar ist... wir haben hier eine Wahl. \nWelche Zyklopin auch immer wir zum Schluss übrig lassen wird höchst wütend und gewalttätig werden. \nDa sollten wir eine kluge Entscheidung treffen.",52,83];
      sisters[15] = ["Gut, die eine ist platt! \nJetzt müssen wir uns nur noch um eine Sorgen machen!",52,64];
      gigaFortress[1] = ["Noch so ne riesige Maschine! \nDas ist dein Fachgebiet, Lance – sag uns, wie wir das bekämpfen!",6];
      gigaFortress[2] = ["Ah, ja, das hier ist das \"Giga-Festung\"-Modell unserer Tunnelbohrmaschinen. \nMein Vater hat vor langer Zeit mal so eine gesteuert. \nEs wurde außer Dienst gestellt, weil es den Boden zu stark erschüttert hat, wodurch Tunnel eingestürzt sind und viele Arbeiter lebendig begraben wurden.",83];
      gigaFortress[3] = ["Wie ihr seht ist sie mit riesigen Tunnelbohrraketen ausgestattet, sowie Säurebomben, um gewisse Gesteinsformen zu zuerätzen. \nIch glaube nicht, dass ich erwähnen muss, wie sich so etwas auf Menschen auswirkt.",61];
      gigaFortress[4] = ["(Vielleicht hätte ich keine fehlerhafte KI-Einheit in dem Teil einbauen sollen. Meine neuen Verbündeten dürfen nie erfahren, dass dieser ganze Wahnsinn meine Schuld ist.)",72];
      gigaFortress[5] = ["(Kommt das hier daher, dass ich keine Risikoabschätzung vor der Modifikation des Panzers durchgeführt hab\'? \nHätte ich vielleicht die Qualitätssicherungsabteilung doch nicht feuern sollen...?         \nUnsinn! Niemand hätte das vorhersehen können.)",66];
      gigaFortress[6] = ["Wir dürfen nicht zulassen, dass die Raketen starten! \nLässt sich das irgendwie stoppen?",52,16];
      gigaFortress[7] = ["Es heilt einfach allen Schaden, den wir ihm zufügen! \nWie sollen wir dieses Teil zerstören?!",107,28];
      gigaFortress[8] = ["Es gibt keinen Trick. \nWir müssen sie nur härter und schneller traktieren, als sie sich reparieren kann.        \n\nDebuffs könnten helfen, um ehrlich zu sein.",88];
      gigaFortress[9] = ["NEIN! NEIN! NEIN! NEIN!",73,25];
      gigaFortress[10] = ["Es schießt!      \nJetzt blüht uns was!",43,103];
      gigaFortress[11] = ["DESHALB TRAU\' ICH KEINEN MASCHINEN!! \nVon denen kommt nie was Gutes!",100];
      gigaFortress[12] = ["Miau!",123];
      gigaFortress[13] = ["M-miauuu...",145];
      gigaFortress[14] = ["*fühlt sich wie im Krieg*",148];
      cocoon[1] = ["*die Geräusche wütender Katzen kommen aus dem Inneren dieses gewaltigen, verflochtenen Baumes*",161];
      cocoon[2] = ["*es klingt, als ob die Katzen sich streiten und sich nicht für einen Kampfplan entscheiden können*",161];
      cocoon[3] = ["*man hört, wie manche Katzen murmeln, dass sie hierfür nicht genug bezahlt werden*",161];
      cocoon[4] = ["*eine mitreißende Rede erklingt, die zu Einigkeit innerhalb der Katzenrasse mahnt*",161];
      cocoon[5] = ["*es ist leise – zu leise*",161];
      cocoon[6] = ["*man hört, wie Katzen sich im Todeskampf verzweifelt gegenseitig zerkratzen*",161];
      cocoon[7] = ["*die Katzen scheinen davon entmutigt, dass ihre Gottheit jetzt für die andere Seite kämpft*",161];
      cocoon[8] = ["Dieser Riesenbaum ist der \"Katzenkönig\"?        \n\nKönnen wir nicht einfach... um ihn rum laufen und dem Kampf aus dem Weg gehen?",45,22];
      cocoon[11] = ["Hmmm, das ist wirklich seltsam – dieser Baum war vor ein paar Tagen noch nicht hier. \n(Könnte das einer dieser... mythischen Wandelnden Bäume sein? \nIch dachte, die wären nur Gruselgeschichten, um die Kinder aus dem Wald draußen zu halten...)",105];
      cocoon[9] = ["Atmet die Sporen nicht ein, oder euch wachsen Pflanzen aus den Ohren und Augen!",94];
      cocoon[10] = ["Womit bespritzt der Baum uns da grad? \nPack\' deine \"Samen\" woanders hin!",69,9];
      cocoon[12] = ["Ah, jetzt versteh ich\'s! \nIch glaube, der Baum verwendet nur Sporen, um die Bewohner des Mystischen Waldes zu beeinflussen! \nKatzen scheinen besonders betroffen zu sein!",106];
      cocoon[13] = ["Hört auf, euch da drin zu verstecken, und bekämpft uns ordentlich!",9,99];
      cocoon[14] = ["Ich wusste schon immer, dass man Katzen nicht vertrauen darf. \nAlle meine Verschwörungstheorien wurden bestätigt, und trotzdem bin ich hier immer noch der \"Spinner\"!",79];
      cocoon[15] = ["NEHMT DAS, IHR DRECKIGEN BIESTER!!",63,3];
      cocoon[16] = ["Wildkatzen können auf komische Ideen kommen, wenn man sie nicht zähmt. \nLasst dies eine Warnung an alle anderen Katzen sein: Tretet keinem Kult bei!",35,92];
      nest[1] = ["*überraschte und zornige Ausrufe erklingen aus den Tiefen dieses Nests*",162];
      nest[2] = ["*man hört, wie Katzen dämonische Worte sprechen und böse Mächte beschwören*",162];
      nest[3] = ["*irgendwas über \"mehr Bomben und Spinnen vorbereiten\"*",162];
      nest[4] = ["*es klingt nach einem inneren Machtkampf*",162];
      nest[5] = ["*die katzen streiten sich nicht mehr, sie haben sich wohl darauf geeinigt, mit Ehre zu sterben*",162];
      nest[6] = ["*man hört, wie Katzen Sudoku begehen*",162];
      nest[13] = ["*die Katze glauben noch immer, dass ihr Kaiserreich wiederaufersteht, aber sind bereit, einen Kompromiss einzugehen und bis dahin in Schande zu leben*",162];
      nest[7] = ["Wie ich sehe hat noch ein Haufen Katzen sich in einem Baum befestigt. \nIch hätte in diesem verfluchten Dungeon schon etwas Schlimmeres erwartet.",65,2];
      nest[8] = ["Ihre Strategie hat sich verbessert – diesmal haben sie Fallen und Waffen vorbereitet!      \nAber es sind immer noch nur Katzen, da gibt es nichts zu befürchten!",82,112];
      nest[9] = ["Sind das Teufelsverehrer?! \nWas hoffen sie denn, damit zu erreichen, solch böse Mächte heraufzubeschwören?",66,54];
      nest[10] = ["Debuffs und Statuseffekte, wie?! \nIhr könnt nicht fair gewinnen, also macht ihr einen auf hinterlistig, hm? \nWenigstens ist unsere Reserve sicher davor!",9,69];
      nest[11] = ["*hört mit diesen beschämenden LARP-Spielchen auf*",130];
      nest[12] = ["Miau...",131];
      poseidon[1] = ["WARUM HABEN STERBLICHE DAS REICH POSEIDONS, DES MEERESGOTTES, BETRETEN?!      \n\nES GIBT NUR EINE MÖGLICHE ERKLÄRUNG – UM VÖLKERMORD AN MEINEN KINDERN ZU BETREIBEN!!",163];
      poseidon[2] = ["STERBLICHE HABEN DIE FISCHE DES MEERES BIS ZUR AUSROTTUNG GEJAGT. \nSIE HABEN DIE FLÜSSE VERSEUCHT UND VIELE SEEN GEFRIEREN LASSEN.         \n\nSOLCHE TATEN SIND UNVERZEIHLICH!!",163];
      poseidon[3] = ["DAS BLUT DER MEERE KLEBT AN EUREN HÄNDEN, STERBLICHE – ICH WERDE JEDE WEHRLOSE LEBENSFORM RÄCHEN, DIE IHR GESCHÄNDET HABT.         \n\nHÖRT MEINE WORTE UND ERZITTERT VOR MEINER MACHT!!",163];
      poseidon[4] = ["ICH SEHE EINE SPUR DER VERWÜSTUNG IN EUREN FUSSSTAPFEN. \nEUER FORTSCHREITEN AUF EINEM SOLCHEN PFAD KANN NICHT GEBILLIGT WERDEN.              \n\nICH WERDE EUCH ZERQUETSCHEN, AUF DASS MEINE KINDER IN FRIEDEN LEBEN KÖNNEN!!",163];
      poseidon[5] = ["KEHRT ZURÜCK ZUM MEER, AUS DEM EURE VORFAHREN STAMMEN!! \nSEID ZU SCHWÄRZESTEN TIEFEN HINWEGGESCHWEMMT!!                  \n\nEURE ÜBERRESTE WERDEN VON EBEN JENEN KREATUREN DES MEERES VERSCHLUNGEN WERDEN, WELCHE IHR ERMORDET HABT!!",163];
      poseidon[6] = ["IST MEINE ZEIT ENDLICH GEKOMMEN? \nERREICHT MEINE HUNDERTTAUSENDJÄHRIGE GESCHICHTE IHR ENDE?!",163];
      poseidon[7] = ["GESCHICHTSBÜCHER WERDEN MEIN ABLEBEN BETRAUERN. \nWAS WERDEN SIE ÜBER EUCH SAGEN, STERBLICHE?!",163];
      poseidon[8] = ["WELCH ERBÄRMLICHE EXISTENZ FÜR EINEN GOTT!! \nTOD WÄRE GEBÜHRENDER ALS SOLCHE SKLAVEREI!!",163];
      poseidon[14] = ["GWHAHAHA, SCHON ZURÜCK? \nIHR WERDET NUR WIEDER FLIEHEN, WIE DIE FEIGLINGE, DIE IHR SEID!",163];
      poseidon[9] = ["DAS ist Poseidon, der Meeresgott?! \nNicht gerade der gutaussehende Mann, den ich mir vorgestellt hab\'.",43,106];
      poseidon[10] = ["Ich dachte, er wäre durchtrainiert und hätte einen gewaltigen Bart.. \nDas ist kein Gott, nur ein riesiger Golem!",16,76];
      poseidon[11] = ["(Könnte sein, dass ich da mit zu beigetragen hab\'. \nVielleicht hätte ich beim Fischen gehen nicht so viel Dynamit einsetzen sollen.)",75];
      poseidon[12] = ["Er ist offensichtlich wahnsinnig, wir sind nicht für all diese Verbrechen verantwortlich!",52,99];
      poseidon[13] = ["Wir müssen ihn jetzt und hier besiegen, um Kiefernhausen zu beschützen.        \n\nEr ist zu gefährlich, um alleine gelassen zu werden.",54,114];
      vulcan[1] = ["ICH HABE EUCH BEREITS ERWARTET – DIE MÖRDER MEINES BRUDERS POSEIDON!!            \n\nER MAG NUN RUHEN, ABER MICH, VULCANUS, VERLANGT ES NOCH IMMER NACH RACHE!!",166];
      vulcan[2] = ["STERBLICHE MÖGEN DIE MEERE AUS WASSER GEMEISTERT HABEN, DOCH NOCH TIEFER LIEGT EIN EWIGES MEER AUS FEUER!!      \nDIE HÖLLE SELBST VERLEIHT MIR KRAFT, UND ICH WERDE IHREN ZORN AUF EUCH HERABBRINGEN!!",166];
      vulcan[3] = ["MIT EUREM SCHICKSAL ZU RINGEN WIRD NUR EUER LEIDEN VERLÄNGERN!! \nLEGT EURE WAFFEN NIEDER UND ERGEBT EUCH MEINEM EWIGEN FEUER!!       \n\nLASST EURE KÖRPER ZU ASCHE VERBRENNEN!!",166];
      vulcan[4] = ["STARK SEID IHR SICHER, ABER ALLES BRENNT BEI AUSREICHEND HOHER TEMPERATUR!!        \n\nSEHEN WIR EINMAL, WIE VIEL HITZE IHR WIDERSTEHEN KÖNNT!!",166];
      vulcan[5] = ["DER STERBLICHE MANN IST EINE PLAGE DIESER ERDE, UND FEUER IST DIE LÄUTERUNG!!        \n\nLASST DIE FLAMMEN VON TAUSEND SONNEN ALLE SPUREN EURER EXISTENZ WEGBRENNEN!!",166];
      vulcan[6] = ["WARUM VERBRENNT IHR NICHT?!\nWARUM KÄMPFT IHR WEITER?!",166];
      vulcan[7] = ["ICH WERDE MICH NICHT VOR STERBLICHEM ABSCHAUM GESCHLAGEN GEBEN!!",166];
      vulcan[8] = ["POSEIDON?? WAS SOLL DAS?! \nWARUM PEINIGST DU MICH SO, BRUDER?!                         \n\nDEIN GEIST KANN MEINE BRENNENDE RACHSUCHT NICHT BERUHIGEN!!",166];
      vulcan[9] = ["ICH, DER MÄCHTIGE VULCANUS, WARD VON MEINEM THRON GESTÜRZT!! \nNUN SEI ICH BEKANNT ALS VULCANUS DER FEIGLING, SKLAVE STERBLICHER!!",166];
      vulcan[10] = ["Hey, wir haben Poseidon nicht ermordet – wir haben ihn gefangen! \nBleibt hier keine gute Tat ungestraft?!",10,69];
      vulcan[11] = ["Oh Mist, interessant, wie unsere Taten uns immer wieder einholen, wie?",51,111];
      vulcan[12] = ["Wissen die Kiefernhausener wirklich nicht, was unter ihrem Dorf liegt?           \n\nWir müssen dieses Monster jetzt aufhalten, bevor alle ausgerottet werden.",55,114];
      vulcan[13] = ["Welch feuriger Zorn! \nSein Kampfgeist brennt wie eine Atomexplosion!",85,25];
      vulcan[14] = ["Wir sind fast da – wir können diesen Kampf gewinnen! \nWeiter so!",34,93];
