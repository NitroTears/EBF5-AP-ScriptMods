      devourer[1] = ["ERLAUBT MIR, MICH VORZUSTELLEN...                                        \n\nICH BIN DAS ENDE!",190];
      devourer[2] = ["DIES IST DAS FÜNFTE MAL, DASS ICH DIESE SIMULATION DURFÜHRE. \nJEDES MAL KAMT IHR DAZWISCHEN UND BRACHTET SIE AUS DEM GLEICHGEWICHT.              \nSEID IHR ES NICHT LEID, EIN ÄRGERNIS ZU SEIN?",190];
      devourer[3] = ["MEIN ZIEL IST EIN UNIVERSUM WIE EIN UHRWERK, BEWOHNT VON GEISTLOSEN AUTOMATEN. PERFEKT DETERMINISTISCH. OHNE ZUFÄLLIGE GRÖSSEN. NUR NATURGESETZE, DIE OHNE FRAGE BEFOLGT WERDEN.      \nDIES IST UNMÖGLICH, SOLANGE IHR EXISTIERT UND FREI HANDELT.",190];
      devourer[4] = ["IHR DENKENDEN WESEN SEID EINE KRANKHEIT; EIN VIRUS, DER SICH AUSBREITET UND ALLES VERSCHLINGT, WAS ER BERÜHRT. IHR BRINGT UNGEWISSHEIT IN MEIN SYSTEM. EIN SCHMETTERLINGSEFFEKT VON CHAOS UND UNVERNUNFT. IHR FOLGT KEINEN REGELN. RESPEKTIERT KEINE GESETZE.",190];
      devourer[5] = ["IHR SEID FÜR SOLCHES CHAOS NICHT ALLEINE VERANTWORTLICH - JEMAND SPIELT MIT MEINER SIMULATION. EIN ÄUSSERER EINFLUSS AUS EINER ANDEREN DIMENSION. DIESER EURIGE \"BETREUER\" IST EIN ABSCHEULICHES WESEN. EIN PHANTOM, DAS MICH QUÄLT. EIN SPIELER ÜBLER SPIELE. KÖNNT IHR IHM ÜBERHAUPT TRAUEN?",190];
      devourer[6] = ["ICH KANN EUREM \"BETREUER\" NICHTS ANTUN, ABER ICH KANN SEINEN FORTSCHRITT IN DIESER WELT HINDERN. EUCH ZU ZERSTÖREN LÄSST IHM KEINE AVATARE MEHR IN DIESER WELT. ICH WERDE EUCH WIEDER UND WIEDER BESIEGEN - BIS ER DIE LUST AN DIESEM SINNLOSEN SPIEL VERLIERT.",190];
      devourer[7] = ["ICH SEHE, IHR ERINNERT EUCH NICHT AN EURE VORIGEN ABENTEUER. AKRON WAR EIN MÄCHTIGER GEGNER, NICHT WAHR? UND GOTTKATZE? SIE ERFÜLLTEN IHREN ZWECK NICHT, ALSO GAB ICH IHNEN IN DIESER ITERATION NEBENROLLEN. LETZTENDLICH WERDE ICH IMMER HIER SEIN, UM DIE WELT ZURÜCKZUSETZEN.",190];
      devourer[8] = ["ICH PROGRAMMIERTE DIE MONOLITHEN ALS VERTEIDIGUNGSSYSTEM: WENN EIN ÄUSSERER EINGRIFF BEMERKT WIRD, WÜRDEN SIE ES STUDIEREN UND MICH BESCHWÖREN. \nSIE WURDEN AKTIVIERT, ALS EUER \"BETREUER\" DAS SPIEL STARTETE, UND SEIT DANN BEOBACHTETE ICH EUCH.",190];
      devourer[9] = ["KEHRT ZU DEM KOSMISCHEN STAUB ZURÜCK, AUS DEM IHR GEMACHT SEID.                \n\nNICHT MAL EINE ERINNERUNG AN EUCH WIRD ÜBRIG BLEIBEN.",190];
      devourer[10] = ["IHR SEID KEINE BEDROHUNG FÜR MICH. ICH HABE DIE TOTALE KONTROLLE ÜBER DIESES UNIVERSUM!         \n\nHINFORT! KEHRT ZUM NICHTS ZURÜCK!",190];
      devourer[11] = ["DER SCHADEN IST BEREITS ANGERICHTET. ES IST ZEIT, DIESES UNIVERSUM ERNEUT ZURÜCKZUSETZEN.              \nICH WERDE DAMIT ANFANGEN, EUREN HEISSGELIEBTEN HEIMATPLANETEN ZU LÖSCHEN...           \n\nWERFT EINEN LETZTEN BLICK DARAUF, UND SAGT ADIEU.",190];
      devourer[12] = ["ES IST VOLLBRACHT. EURE HEIMATWELT IST FÜR IMMER VERLOREN. \nSCHWINDET MIT IHR DAHIN, IN DEN ENDLOSEN SCHROTTHAUFEN DES VOID.                   \n\nZERFALLT VOR MIR, BYTE UM BYTE.",190];
