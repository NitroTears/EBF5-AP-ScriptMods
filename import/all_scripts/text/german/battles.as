      cyclops[4] = ["AFFE KNOCHEN KNUSPER UND LECKER!! \nMACHT GUT BUTTER FÜR STEIN BROT!!",160];
      cyclops[5] = ["WARUM DUMM SCHIMPS NICHT BRECHEN?!            \n\nZYKLOP NICHT HAUEN STARK GENUG?!",160];
      cyclops[6] = ["WARUM ZYKLOP SEIN MÜD UND WEH?           \n\nMICKRIG WAFFE KÖNNEN NICHT MACHEN...",160];
      cyclops[7] = ["ZYKLOP AUGE WEH.  \nBRAUCHEN AUGENTROPFEN.",160];
      cyclops[8] = ["ZYKLOP STERBEN MIT ALLE REUE...",160];
      cyclops[9] = ["ZYKLOP ARBEITEN HART FÜR TEAM!! \nVERSTEHEN STÄRKE VON ZUSAMMEN!",160];
      cyclops[16] = ["ERBÄRMLICH AFFEN ZURÜCK FÜR MEHR PRÜGELN? \nRENN WEG WIE HUNDE LETZTE MAL!",160];
      cyclops[10] = ["Das ist ziemlich viel Gewicht, dass er da so einfach rumwirft!",55,24];
      cyclops[11] = ["Er ist groß, aber sieht echt dumm aus! \nWir müssen nur schnell auf seine Angriffe reagieren!",16];
      cyclops[12] = ["Wie spricht der Zyklop eigentlich ohne Mund? \nRedet der aus dem Hintern?",29];
      cyclops[13] = ["Wir sind keine Affen, du blöder Gorilla!",10,40];
      cyclops[14] = ["Er sieht ziemlich verletzt aus! \nEr wird jetzt härter zuschlagen, aber der Sieg ist nahe!",46];
