      lance[16] = ["Während ich an meiner Forschung schufte, tun die schwächlichen Regierungen der Welt nichts, um die Existenzbedrohnungen anzugehen, die vor uns liegen. \nDie Welt muss unter einem starken Führer vereint werden, und gemeinsam auf ein einzelnes Ziel zuarbeiten.",65];
      lance[17] = ["Warum stellt ihr euch in meinen Weg? Warum versteht ihr nicht, dass meine Sache die gerechte ist? \nIch will die Welt unter einer Ideologie vereinigen - nur so kann die Menschheit überleben.",69];
      lance[21] = ["DIE WELT IST VERLOREN, WENN IHR MICH NICHT VORANSCHREITEN LASST!!",70];
      lance[18] = ["(S-sie schaffen es wirklich, sie zu beschädigen?! \nDas sollte unmöglich sein!)",85];
      lance[19] = ["Wooooaaahhhhh...            \n\n(Vielleicht sollte ich sie leben lassen und mit ihnen verhandeln... aber vermutlich kann ich diesen Kampf immer noch gewinnen.)",80];
      lance[20] = ["Ich-ich kann so nicht kämpfen! \nEure Taktik ist billig!          \n\nVielleicht können wir doch verhandeln!",87];
      lance[22] = ["Neon-Walküre! \nRäche mich, meine Liebe!",74];
      lance[23] = ["Neon-Walküre! \nNEEEEEEEEEEEEEIIIIIIN!!!               \n\nIhr habt sie umgebracht! ",78];
      lance[24] = ["Kommt, ich zeige euch Neon-Walküres mobiles Atomschlagsystem!             \n\nDas ist euer Ende - ihr könnt nicht überleben!",63];
      lance[31] = ["JAAAAAAA!!!                \n\nWUNDERVOLL!!!",80];
      lance[25] = ["Kommt schon, Leute, er wird müde. \nMachen wir ihn endgültig fertig!",52];
      lance[26] = ["Ich mach dir dein Auto kaputt für das Hit-and-Run vorhin!           \n\nWo hast du überhaupt fahren gelernt!?",10];
      lance[27] = ["Glaubt er wirklich, davon beeindruckt uns irgendwas? \nMit dem ganzen Technobabble streichelt der doch nur sein eigenes Ego!",9,39];
      lance[28] = ["Das nenn\' ich mal Selbstbetrug! \nDer Typ labert Unsinn.",57,112];
      chibi[1] = ["Huch,Was ist denn das für ein Ritterchen? \nWill es uns bekämpfen?",22,76];
      chibi[2] = ["Oh, das ist ja ein Mädchen!",16,32];
      chibi[3] = ["Sie ist energetisch, so viel lasse ich ihr.",2,33];
      chibi[4] = ["Sie ist weggerannt, wie? \nWas hatte sie überhaupt für ein Problem mit uns?",19,51];
      chibi[5] = ["Es ist schon wieder das Ritterchen! \nUnd es sieht so aus, als hätte sie diesmal bessere Ausrüstung!",61,1];
      chibi[6] = ["Sieht so aus, als ob sie auch ein Held sein will. \nIch wette, sie ist nur genervt, dass wir die Hauptcharaktere sind.",21,117];
      chibi[7] = ["Geh woanders Monster bekämpfen, kleiner Ritter, die Gegend gehört uns! \nDu kannst uns nicht unsere EXP wegnehmen!",9];
      chibi[8] = ["Sie hat jetzt Zauber! \nSie wird stärker, so wie ein Spielercharakter!",76,16];
      chibi[9] = ["Und weg ist sie.       \n\nIch bin mir sicher, wir kriegen noch mehr von ihr zu sehen....",52,92];
      chibi[10] = ["Woah, Chibi Knight ist zurück und sie ist hat epische neue Rüstung! \n(Mir würde sie nicht passen, aber vielleicht NoLegs...?)",16,103];
      chibi[11] = ["SIE KANN SO LEICHT MAMMUTE BEFEHLIGEN?! \nIch glaube, wir haben sie wirklich unterschätzt!",24,85];
      chibi[12] = ["Lass das, du Möchtegern-Meta-Kn*ght!",9,40];
      chibi[13] = ["Ich glaube, sie versteht inzwischen, wie Elementarresistenzen funktionieren... \nSie könnte versuchen, unsere Schwächen auszunutzen!",52,82];
      chibi[14] = ["In nur ein paar Tagen ist sie von einem Noob zu einem richtigen Boss geworden. \nVielleicht wird sie bald ein echter Spielercharakter?",22,82];
      chibi[15] = ["Oh... anscheinend war sie doch kein Spielercharakter. \nIch glaube ja nicht, dass wir sie jetzt nochmal wiedersehen.           \n\nWir hätten sie vermutlich nicht tötet brauchen...",7,66];
      hydra[1] = ["Okay, wir haben diesen Boss schon mal besiegt. \nJetzt hat sie drei Köpfe, aber ansonsten ist alles gleich!",52,4];
      hydra[2] = ["Immer auf die Kleinen, wie? Knabber\' doch jemanden an, der dir gewachsen ist!",40,69];
      hydra[3] = ["Er ist so viel schneller, wenn man ihn abhackt! \n(Ein Hals ist wohl eine Last, wenn man ein Skelett ist.)",99,9];
      hydra[4] = ["HALT STILL UND HÖR AUF, RUMZUFLIEGEN, OKAY?!",30,120];
      hydra[5] = ["Es kann verlorene Köpfe respawnen?! \nNich fair!",10,100];
      hydra[6] = ["Ich nehme an, wir müssen einfach alle Köpfe ungefähr gleichzeitig töten. \nDas wird schwer bei unserer schrecklichen Koordination.",66,95];
      hydra[7] = ["Die Standardgleichung gilt immer noch: \nUNTOT + LICHT = TOT.",53,113];
      hydra[14] = ["D-Drachen?!        \n\nNee, die hier sind etwas anders als normale Drachen...",6,35];
      hydra[15] = ["Das ist mal ziemlich scharfer Feueratem...",54,24];
      hydra[16] = ["Ahhh, eine Hydra ist anders als ein Drache. \nDer Körper ist außerhalb des Bildschirms, aber alle Köpfe gehören zum selben Monster, und können respawnen, wenn man sie tötet!",66,36];
      hydra[8] = ["Sein Biss fühlt sich so an, wie wenn tausend Glasscherben dich gleichzeitig schneiden!",24];
      hydra[9] = ["Lichtangriffe hat er erst verwendet, seit wir ihn abgehackt haben! \nIst das jetzt plötzlich ein Lichtmonster?!",16,105];
      hydra[10] = ["Ich glaube, die Elementarresistenz der Köpfe ändert sich, wenn man sie abhackt. \nDas macht es ziemlich kompliziert, Schaden zu maximieren!",66,35];
      hydra[11] = ["Haben wir sie endlich fertiggemacht? \nJetzt respawnt sie nicht nochmal, oder? \nBesser tut sie das nicht...",109,17];
      hydra[12] = ["*fragt sich ob Statuseffekte funktionieren*",135];
      hydra[13] = ["*verstört von der Aussicht, in einem Bissen gegessen zu werden*",148];
      cyclops[1] = ["ZYKLOP JETZT RUHEN.          \n\nAFFEN KÖNNEN GEHEN HEIM ODER WERDEN ROTE FARBE FÜR HÖHLE!!",160];
