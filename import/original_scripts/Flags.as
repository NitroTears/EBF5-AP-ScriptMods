      public static function isBunkerBattle() : Boolean
      {
         return battleEvent >= 47 && battleEvent <= 54 || battleEvent == TWO_PLAYER_RUSH || battleEvent == THREE_PLAYER_RUSH;
      }
      
      public static function isEBF3or4() : Boolean
      {
         return battleEvent >= 51 && battleEvent <= 54 || battleEvent == THREE_PLAYER_RUSH;
      }
      
      public static function isVideoGameBattle() : Boolean
      {
         return isBunkerBattle() || battleEvent == 32 || battleEvent >= 35 && battleEvent <= 46;
      }
      
      public static function isArcadeBattle() : Boolean
      {
         return battleEvent == 32 || battleEvent >= 35 && battleEvent <= 46;
      }
      
      public static function checkSeason() : *
      {
         if(!date)
         {
            date = new Date();
         }
         if(date.month == 9 || date.month == 10 && date.date <= 7)
         {
            HALLOWEEN.quantity = 1;
         }
         else
         {
            HALLOWEEN.quantity = 0;
         }
         if(date.month == 11 || date.month == 0 && date.date <= 7)
         {
            CHRISTMAS.quantity = 1;
         }
         else
         {
            CHRISTMAS.quantity = 0;
         }
         if(date.month == 1 && date.date <= 21 && date.date >= 7)
         {
            VALENTINES.quantity = 1;
         }
         else
         {
            VALENTINES.quantity = 0;
         }
         if(date.month == 2 && date.date >= 14 || date.month == 3)
         {
            EASTER.quantity = 1;
         }
         else
         {
            EASTER.quantity = 0;
         }
         if(date.month == 0 && date.date >= 18 || date.month == 1 && date.date <= 25)
         {
            LUNAR.quantity = 1;
         }
         else
         {
            LUNAR.quantity = 0;
         }
      }
      
