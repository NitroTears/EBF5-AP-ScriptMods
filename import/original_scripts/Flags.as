         {
            SUPER_BOSSES_DEFEATED.quantity = 1;
         }
         if(!DUNGEONS_UNLOCKED.quantity)
         {
            SUPER_BOSSES_DEFEATED.quantity = 0;
         }
         if(Items.hammer.quantity)
         {
